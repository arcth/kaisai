
page{
  min-height: 100%;
  height:100%;
  
}
.scrollPage{
	height: 100vh;
	margin-bottom: calc(100rpx + env(safe-area-inset-bottom) / 2);
}
.login-page-container{
	background-color: #fff;
	padding-top: 292rpx;
	height: 100%;
}
.login-page-logo{
	margin:0 auto 70rpx;
	width: 166rpx;
	height: 158rpx;
	background: url('data:image/png;base64,')  ;
	background-repeat: no-repeat;
	background-position: center;
	background-size: 100%;
}
.kaisai-text{
	display: block;
	margin: 0 auto;
	width: 132rpx;
	height: 64rpx;
}
.kaisai-line{
	display: block;
	margin: 28rpx auto;
	width: 492rpx;
	height: 8rpx;
}
.login-line-text{
	display: block;
	margin: 0 auto;
	width: 166rpx;
	height: 20rpx;
}
.bottom-btn-container{
	position: fixed;
	width: 100%;
	bottom: 0;
}
.login-bottom{
	width: 540rpx;
	height: 84rpx;
	line-height: 84rpx;
	background: #fff;
	color: #004cff;
	font-size: 32rpx;
	font-weight: 500;
	border: 1px solid #004cff;
	border-radius: 42rpx;
}
.login-bottom-text{
	display: block;
	margin: 52rpx auto;
	width: 280rpx;
	height: 28rpx;
}