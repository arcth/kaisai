.share-bg{
	width: 100%;
	height: 100%;
	position: fixed;
	z-index: -1;
}
.auto-page{
	width: 100%;
	height: auto;
	overflow-y: auto;
	padding: 100rpx 50rpx;
}
.match-text-container{
	margin-bottom: 40rpx;
	min-height: 140rpx;
}
.match-title{
	display: -webkit-box;
	font-size: 64rpx;
	font-weight: bold;
	color: #fff;
	text-align: center;
	margin-bottom: 18rpx;
	-webkit-box-orient: vertical;    
	-webkit-line-clamp: 2;    
	overflow: hidden;
}
.match-sub-title{
	color: #fff;
	font-size: 30rpx;
	text-align: center;
}
.match-user-info{
	display: flex;
	justify-content: space-around;
	margin: 40rpx 0 0 0;
}
.match-group-item{
	color: #fff;
}

.group-name{
	
}

.match-user-info image{
	display: block;
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
}
.btn-group{
	margin-top: 100rpx;
	text-align: center;
	color: #fff;
}
.btn-group-item{
	width: 540rpx;
	height: 84rpx;
	line-height: 84rpx;
	color: #fff;
	border: 2rpx solid #fff;
	text-align: center;
	border-radius: 42rpx;
	margin-bottom: 30rpx;
	background-color: transparent;
}
.vb-img{
	display: block;
	width: 364rpx;
	height: 180rpx;
	margin: 200rpx  auto 0;
}